{
  "name": "react-boilerplate",
  "version": "1.1.0",
  "private": true,
  "description": "A React template with custom webpack configuration and support for code splitting. ",
  "main": "src/index.js",
  "keywords": [
    "react",
    "boilerplate",
    "template",
    "webpack",
    "redux",
    "eslint",
    "prettier",
    "code-splitting",
    "react-loadable",
    "react-router-dom"
  ],
  "author": "Ritesh Patil",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/riteshsp2000/react-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/riteshsp2000/react-boilerplate.git"
  },
  "scripts": {
    "build:dev": "webpack --env development",
    "build:prod": "webpack -p --env production",
    "develop": "webpack-dev-server --env development",
    "start": "node server/server.js",
    "prettier:fix": "npx prettier --write .",
    "prettier:check": "npx prettier --check .",
    "lint:fix": "npx eslint --fix '**/*.js'",
    "lint:find": "npx eslint '**/*.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --colors"
  },
  "dependencies": {
    "history": "^5.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-loadable": "^5.5.0",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.1.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-react": "^6.24.1",
    "brotli-webpack-plugin": "^1.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "6.3.2",
    "css-loader": "^4.3.0",
    "dotenv": "^10.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "eslint": "^7.10.0",
    "eslint-config": "^0.3.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-loader": "^1.3.1",
    "html-webpack-plugin": "^4.5.0",
    "jest": "^26.6.3",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.0.2",
    "sass-loader": "^10.0.2",
    "style-loader": "^1.2.1",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.44.2",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
